{
  "name": "odysseat",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "auth:generate": "npx @better-auth/cli generate --config ./scripts/auth-config.ts --output ./src/server/db/schema/auth-schema.ts",
    "build": "next build",
    "check": "cross-env SKIP_ENV_VALIDATION=true next lint && tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:scrape": "tsx --env-file=.env scripts/scrape-db",
    "db:seed": "tsx --env-file=.env scripts/seed-db",
    "db:studio": "drizzle-kit studio",
    "dev": "next dev --turbo",
    "format:check": "cross-env SKIP_ENV_VALIDATION=true prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "cross-env SKIP_ENV_VALIDATION=true prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "lint": "cross-env SKIP_ENV_VALIDATION=true next lint",
    "lint:fix": "cross-env SKIP_ENV_VALIDATION=true next lint --fix",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "cross-env SKIP_ENV_VALIDATION=true tsc --noEmit"
  },
